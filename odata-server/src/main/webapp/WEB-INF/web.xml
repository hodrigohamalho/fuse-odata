<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Odata server application example</display-name>

  <servlet>
		<servlet-name>Demo</servlet-name>
		<servlet-class>com.example.demo.service.Servlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>Demo</servlet-name>
		<url-pattern>/Demo.svc/*</url-pattern>
	</servlet-mapping>
</web-app>
